// Largest element in an array


class Compute {
    
    public int largest(int arr[], int n)
    {
    //     Arrays.sort(arr);
    // //   for(int i=0;i<n;i++){
    // //       return arr[n-1];
    // //   }
    //   return arr[n-1];
    // }
    int ans=arr[0];
    for(int i=1;i<arr.length;i++){
       // if(ans>arr[n]) return arr[n];
      
        if(arr[i]>ans){
            ans=arr[i];
    //     }else{
    //         i++;
    //     }
    // }
    // return ans;
    }
}
return ans;
}
